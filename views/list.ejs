<%- include("header") -%>
    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>
    <div class="box">
        <% for (let i = 0; i < todoList.length; i++) { %>
          <div class="item">
            <input type="checkbox">
            <p><%= todoList[i] %><p>
          </div>
        <% } %>
        <form class="item" action"/"<%= listTitle %> method="post">
          <input type="text" name="nextItem" placeholder="New Item" autocomplete="off">
          <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>
    
    
  </body>
  <footer>
    <p>This is a mediocre todo list made by Jason Hamel</p>
    <p>Just a learning tool for EJS</p>
  </footer>
</html>

